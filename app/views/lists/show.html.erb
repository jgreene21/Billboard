<h1>Billboard Lists</h1>

<h2> <%= @list.name %></h2>
 <% if @songs.any? %>
  <% @songs.each do |song| %>
    <p><%= song.name %></p>
   <%= link_to 'Remove', remove_list_song_path(@list, song), method: :delete %>
  <br/>
  <% end %>
<% end %>
<br/>
<%= link_to 'Add Song', new_list_song_path(@list), class: "ui secondary button" %><%= link_to 'Back', lists_path, class: "ui secondary button" %>
<br></br> 


